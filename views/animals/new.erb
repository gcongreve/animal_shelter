

<form action="/animals/" method="post">

  <label for="name">Name: </label>
  <input id="name" type="text" name="name">
  <br>

  <label for="species">Species: </label>
  <input id="species" species="text" name="species">
  <br>
  <label for="type">Type: </label>
  <input id="type" type="text" name="type">

  <br>
  <label for="date_admitted">Date Admitted: </label>
  <input id="date_admitted" type="text" name="date_admitted">

  <br>
  <label for="healthy">Healthy</label>
  <select class="healthy" name="healthy">
    <option value="t">True</option>
    <option value="f">False</option>
  </select>

  <br>
  <label for="Trained">Trained</label>
  <select class="trained" name="trained">
    <option value="t">True</option>
    <option value="f">False</option>
  </select>


  <input type="submit" value="Create Animal">

</form>
